/*按钮样式*/
.up {
    width: 54px;
    height: 54px;
    background: url(https://wx3.sinaimg.cn/mw690/00617AGFgy1fyd845i2f4j301i0300sj.jpg) no-repeat 0 0;
}

.report-file {
    display: block;
    position: relative;
    width: 120px;
    height: 28px;
    margin-top: 5px;
    overflow: hidden;
    border: 1px solid #428bca;
    background: none repeat scroll 0 0 #428bca;
    color: #fff;
    cursor: pointer;
    text-align: center;
    float: left;
    margin-right: 5px;
}
.report-file span {
    cursor: pointer;
    display: block;
    line-height: 28px;
}
.file-prew {
    position: absolute;
    top: 0;
    left:0;
    width: 120px;
    height: 30px;
    font-size: 100px;
    opacity: 0;
    filter: alpha(opacity=0);
    cursor: pointer;
}

