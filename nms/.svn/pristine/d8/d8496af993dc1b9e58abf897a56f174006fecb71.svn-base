	ReplaceConfigMapper rMapper;
	public List<ReplaceConfig> getAllConfigs(ReplaceConfig replaceConfig) {
		// TODO Auto-generated method stub
		return rMapper.getAllConfigs(replaceConfig) ;
	}
	public int getCount() {
		// TODO Auto-generated method stub
		return rMapper.getCount();
	}
