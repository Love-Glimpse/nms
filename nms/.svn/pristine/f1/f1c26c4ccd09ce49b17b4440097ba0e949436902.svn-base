@Service
public class KeywordReplyServiceImpl {
	@Autowired
	KeywordReplyMapper kMapper;

	public List<PartnerKeyword> getKeywordReply(Integer start_rows, Integer rows, int partner_id) {
		// TODO Auto-generated method stub
